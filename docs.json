                        "pages": [
                          "integration/online-shop/integration-by-payment-type/wechat/wechat-in-app-payments",
                          "integration/online-shop/integration-by-payment-type/wechat/wechat-jsapi-payment",
                          "integration/online-shop/integration-by-payment-type/wechat/wechat-mini-programs",
                          "integration/online-shop/integration-by-payment-type/wechat/wechat-pay-h5",
                          "integration/online-shop/integration-by-payment-type/wechat/wechat-web-qr-payment"
