                  {
                    "group": "Integration by Payment Type",
                    "pages": [
                      {
                        "group": "Alipay",
                        "pages": [
                          "integration/online-shop/integration-by-payment-type/alipay/alipay-in-app",
                          "integration/online-shop/integration-by-payment-type/alipay/alipay-wap-h5-payments",
