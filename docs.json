{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "QFPay Developer Centre",
  "colors": {
    "primary": "#005FCC",
    "light": "#4EA8FF",
    "dark": "#004299"
  },
  "favicon": "/images/logo.png",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "QFPay Website",
          "icon": "book-open-cover",
          "href": "https://www.qfpay.global"
        }
      ]
    },
    "languages": [
      {
        "language": "en",
        "tabs": [
          {
            "tab": "Home",
            "groups": [
              {
                "group": "Getting Started",
                "pages": [
                  "quickstart",
                  "development"
                ]
              }
            ]
          },
          {
            "tab": "Integration",
            "groups": [
              {
                "group": "Preparation",
                "pages": [
                  "integration/preparation/introduction",
                  "integration/preparation/environments"
                ]
              },
              {
                "group": "API Reference",
                "pages": [
                  "integration/api-reference/api-usage-guidelines",
                  "integration/api-reference/currencies",
                  "integration/api-reference/paytypes",
                  "integration/api-reference/request-format",
                  "integration/api-reference/response-format",
                  "integration/api-reference/signature-generation",
                  "integration/api-reference/status-codes"
                ]
              }
            ]
          },
          {
            "tab": "In-Store Payments",
            "groups": [
              {
                "group": "POS API",
                "pages": [
                  "integration/in-store/pos-api/app-call-app",
                  "integration/in-store/pos-api/ecr"
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  "logo": "/logo.png",
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "ai": {
    "enabled": true
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  },
  "appearance": {
    "default": "light"
  }
}
